template(this.elem === 'a') (function() {
    return '<a href="' + this.href + '">' + this.text + '</a>';
});

template(this.elem === 'div') (function() {
    return '<div>' + this.body + '</div>';
});

template(this.elem === 'div' && this.colour === 'blue') (function() {
    return '<div class="blue">' + this.body + '</div>';
});
